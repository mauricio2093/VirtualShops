{
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "extends": [
    "eslint:recommended",
    "prettier",
    "airbnb-base"
  ],
  "env": {
    "es6": true,
    "node": true,
    "jest": true,
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "rules": {
    "no-console": "warn",
    "linebreak-style": 0
  }
}
